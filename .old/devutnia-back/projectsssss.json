{
  "sourceRoot": "packages/devutnia-back/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/packages/devutnia-back",
        "main": "packages/devutnia-back/src/main.ts",
        "index": "packages/devutnia-back/src/front/index.html",
        "tsConfig": "packages/devutnia-back/tsconfig.app.json",
        "assets": [
          "packages/devutnia-back/src/assets",
          "packages/devutnia-back/src/front"
        ]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "packages/devutnia-back/src/environments/environment.ts",
              "with": "packages/devutnia-back/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "devutnia-back:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "devutnia-back:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/devutnia-back/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/devutnia-back/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}

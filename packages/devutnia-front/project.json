{
  "name": "devutnia-front",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/devutnia-front",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "packages/devutnia-front",
        "outputPath": "dist/packages/devutnia-front"
      },
      "configurations": {
        "development": {
          "outputPath": "packages/devutnia-front"
        },
        "production": {}
      },
      "dependsOn": ["build-custom-server"]
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "devutnia-front:build",
        "dev": true,
        "customServerTarget": "devutnia-front:serve-custom-server"
      },
      "configurations": {
        "development": {
          "buildTarget": "devutnia-front:build:development",
          "dev": true,
          "customServerTarget": "devutnia-front:serve-custom-server:development"
        },
        "production": {
          "buildTarget": "devutnia-front:build:production",
          "dev": false,
          "customServerTarget": "devutnia-front:serve-custom-server:production"
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "devutnia-front:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/devutnia-front/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/devutnia-front/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "build-custom-server": {
      "executor": "@nrwl/js:tsc",
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/devutnia-front",
        "main": "packages/devutnia-front/server/main.ts",
        "tsConfig": "packages/devutnia-front/tsconfig.server.json",
        "clean": false,
        "assets": []
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "serve-custom-server": {
      "executor": "@nrwl/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "devutnia-front:build-custom-server"
      },
      "configurations": {
        "development": {
          "buildTarget": "devutnia-front:build-custom-server:development"
        },
        "production": {
          "buildTarget": "devutnia-front:build-custom-server:production"
        }
      }
    }
  },
  "tags": []
}

{
  "name": "devutnia",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "dev:server": "nx run test-server:serve",
    "back:run": "nx run devutnia-back:serve",
    "back:build": "npm run front:back:link && nx run devutnia-back:build",
    "back:pack": "npm run front:back:link && cd packages/devutnia-back && npm run pack",
    "front:run": "cd packages/devutnia-front && npm run serve",
    "front:build": "cd packages/devutnia-front && npm run build",
    "front:back:link": "nx build devutnia-front --outputPath=packages/devutnia-back/src/front",
    "devutnia:bundle": "cd packages/devutnia-back && npm run bundle",
    "devutnia:exe": "./dist/bins/devutnia-exe"
  },
  "private": true,
  "dependencies": {
    "core-js": "^3.6.5",
    "express": "^4.18.1",
    "localforage": "^1.10.0",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nrwl/esbuild": "^15.1.1",
    "@nrwl/eslint-plugin-nx": "15.1.1",
    "@nrwl/express": "15.1.1",
    "@nrwl/jest": "15.1.1",
    "@nrwl/js": "15.1.1",
    "@nrwl/linter": "15.1.1",
    "@nrwl/node": "15.1.1",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "^15.1.1",
    "@nrwl/web": "15.1.1",
    "@nrwl/webpack": "15.1.1",
    "@types/express": "4.17.13",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "28.1.1",
    "@types/node": "18.7.1",
    "@types/yargs": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "babel-jest": "28.1.1",
    "email-validator": "^2.0.4",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "glob": "^8.0.3",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^2.0.6",
    "inquirer": "^8.0.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
